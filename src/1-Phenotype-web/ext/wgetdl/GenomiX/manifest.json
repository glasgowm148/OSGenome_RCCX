{
    "manifest_version": 2,
    "name": "GenomiX Plugin",
    "description": "This extension will add a link to this page into your GenomiX Notebook",
    "version": "0.4",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}